{
    "name": "do87/komposer-mirror",
    "description": "Composer lazy mirror",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dean Oren"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "test": "phpunit"
    }

}
